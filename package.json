{
  "name": "survey_h5",
  "version": "1.0.0",
  "private": true,
  "description": "survey h5",
  "license": "ISC",
  "scripts": {
    "start": "./node_modules/.bin/nodemon  --watch gulpfile.js --watch package.json --exec gulp",
    "dev": "./node_modules/.bin/webpack --env=dev --progress --colors --display-error-details",
    "prod": "./node_modules/.bin/webpack   --env=production --progress --colors",
    "test": "./node_modules/.bin/webpack   --env=test --progress --colors",
    "hot": "./node_modules/.bin/webpack-dev-server --env=hot --inline --hot",
    "mock": "./node_modules/.bin/webpack --env=mock --progress --colors --display-error-details",
    "mockhot": "./node_modules/.bin/webpack-dev-server --env=mockhot --inline --hot",
    "mockp2r": "./node_modules/.bin/webpack --env=mockp2r --progress --colors --display-error-details",
    "mockp2rhot": "./node_modules/.bin/webpack-dev-server --env=mockp2rhot --inline --hot",
    "devhot": "npm run dev;npm run hot",
    "prodhot": "npm run prod;npm run hot",
    "defaultmockhot": "npm run mock;npm run mockhot",
    "remmockhot": "npm run mockp2r;npm run mockp2rhot"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "iosselect": "0.0.6",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mockjs": "^1.0.0",
    "object-assign": "^3.0.0",
    "path": "^0.12.7",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-loader": "^1.3.3",
    "postcss-px2rem": "^0.3.0",
    "precss": "^1.4.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.5.1",
    "yargs": "^4.7.1"
  },
  "dependencies": {
    "echarts": "^3.6.2",
    "scrollload": "^3.2.1",
    "swiper": "^3.4.2",
    "touchjs": "^0.2.14"
  }
}
